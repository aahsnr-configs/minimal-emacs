# -*- mode: snippet -*-
# name: Setup.py
# key: __setup
# --
from setuptools import setup, find_packages

setup(
    name="${1:package_name}",
    version="${2:0.1.0}",
    packages=find_packages(),
    install_requires=[
        $0
    ],
    entry_points={
        'console_scripts': [
            '$1 = $1.main:main',
        ],
    },
)
