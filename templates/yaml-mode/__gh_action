# -*- mode: snippet -*-
# name: Github Action
# key: __gh_action
# --
name: ${1:CI}

on:
  push:
    branches: [ "main" ]
  pull_request:
    branches: [ "main" ]

jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - name: Run script
        run: echo Hello World
